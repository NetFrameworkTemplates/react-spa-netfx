{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "npm run build",
    "dev": "rimraf wwwroot/ && node scripts/dev.js",
    "start": "react-scripts-ts start",
    "dtos": "typescript-ref",
    "build": "rimraf wwwroot/ && react-scripts-ts build && (move build wwwroot || mv build wwwroot)",
    "publish": "npm run build",
    "test": "react-scripts-ts test --env=jsdom --watchAll",
    "test-coverage": "npm test -- --coverage",
    "eject": "react-scripts-ts eject"
  },
  "proxy": "https://localhost:5001/",
  "dependencies": {
    "@servicestack/client": "^1.0.31",
    "@servicestack/react": "^1.0.5",
    "bootstrap": "^4.3.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts-ts": "^3.1.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-test-renderer": "^16.8.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "gulp": "^4.0.2",
    "node-fetch": "^2.6.7",
    "react-test-renderer": "^16.8.6",
    "typescript": "^3.5.3"
  }
